{
  "name": "lledo-industries",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:fresh": "powershell -Command \"Stop-Process -Name node -Force -ErrorAction SilentlyContinue; Start-Sleep -Seconds 1; npm run dev\"",
    "dev:kill": "powershell -Command \"Stop-Process -Name node -Force -ErrorAction SilentlyContinue\"",
    "build": "npm run i18n:strict && prisma db push --skip-generate --accept-data-loss && prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:i18n": "eslint src --ext .tsx,.ts --rule 'react/jsx-no-literals: error'",
    "i18n:validate": "npx tsx scripts/validate-i18n.ts",
    "i18n:strict": "npx tsx scripts/i18n-strict-validate.ts",
    "i18n:strict:fix": "npx tsx scripts/i18n-strict-validate.ts --fix",
    "i18n:types": "npx tsx scripts/generate-i18n-types.ts",
    "i18n:check": "npm run i18n:strict && npm run lint:i18n",
    "i18n:report": "npm run i18n:strict:fix",
    "prebuild": "npm run i18n:strict",
    "type-check": "tsc --noEmit",
    "postbuild": "next-sitemap",
    "translate": "node scripts/translate.js",
    "translate:i18n": "node scripts/translate-i18n.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "npx tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset"
  },
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@google/model-viewer": "^4.1.0",
    "@hookform/resolvers": "^3.3.2",
    "@prisma/client": "^6.19.2",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@vercel/analytics": "^1.1.1",
    "@vercel/blob": "^2.3.0",
    "autoprefixer": "^10.4.16",
    "bcryptjs": "^3.0.3",
    "clsx": "^2.0.0",
    "contentlayer": "^0.3.4",
    "date-fns": "^3.0.6",
    "framer-motion": "^10.16.16",
    "gray-matter": "^4.0.3",
    "i18next": "^25.6.0",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.548.0",
    "next": "^14.2.33",
    "next-auth": "^5.0.0-beta.30",
    "next-seo": "^6.4.0",
    "next-sitemap": "^4.2.3",
    "nodemailer": "^7.0.13",
    "otplib": "^13.2.1",
    "postcss": "^8.4.32",
    "prisma": "^6.19.2",
    "puppeteer": "^24.37.5",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.48.2",
    "react-i18next": "^16.2.1",
    "react-markdown": "^10.1.0",
    "resend": "^6.9.1",
    "sharp": "^0.33.1",
    "tailwindcss": "^3.4.0",
    "three": "^0.182.0",
    "typescript": "^5.3.3",
    "zod": "^3.22.4",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/nodemailer": "^6.4.22",
    "@types/qrcode": "^1.5.6",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.2.33",
    "tsx": "^4.21.0"
  }
}
